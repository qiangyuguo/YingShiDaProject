  <connectionStrings />
  <system.web>

    <!--上传大文件配置开始（1） 最大只能上传2G文件-->
    
    <httpModules>
      <add name="HttpUploadModule" type="Maticsoft.Common.MaxFileUpload,Maticsoft.Common"/>
    </httpModules>
    
    <!--此处限制在500M-->
    <httpRuntime maxRequestLength="512000" executionTimeout="3000"/>
    <!--上传大文件配置结束 最大只能上传2G文件-->

    <compilation debug="true" />
    <!--
            通过 <authentication> 节可以配置
            安全身份验证模式，ASP.NET 
            使用该模式来识别来访用户身份。 
        -->
    <authentication mode="Windows" />
    <!--
            如果在执行请求的过程中出现未处理的错误，
            则通过 <customErrors> 节
            可以配置相应的处理步骤。具体而言，
            开发人员通过该节可配置要显示的 html 错误页，
            以代替错误堆栈跟踪。

        <customErrors mode="RemoteOnly" defaultRedirect="GenericErrorPage.htm">
            <error statusCode="403" redirect="NoAccess.htm"/>
            <error statusCode="404" redirect="FileNotFound.htm"/>
        </customErrors>
        -->
    <pages />
    <sessionState timeout="60" />
  </system.web>
  <system.webServer>    
    <directoryBrowse enabled="true" />
    <defaultDocument>
      <files>
        <add value="Login.aspx" />
      </files>
    </defaultDocument>
    <staticContent>
      <mimeMap fileExtension=".cfg" mimeType="application/octet-stream" />
      <mimeMap fileExtension=".apk" mimeType="application/x-zip-compressed" />
    </staticContent>
    <security>
      <requestFiltering>
        <!--上传大文件配置开始（2） 最大只能上传2G文件 500M : 524288000 -->
        <requestLimits maxAllowedContentLength="524288000" ></requestLimits>
        <!--上传大文件配置结束（2） 最大只能上传2G文件-->        
        <fileExtensions>
          <add fileExtension=".png" allowed="true" />
        </fileExtensions>
      </requestFiltering>      
    </security>
    <modules>
      <add name="MaxFileUpload" type="Maticsoft.Common.MaxFileUpload,Maticsoft.Common"/>
    </modules>
    <validation validateIntegratedModeConfiguration="false" />
  </system.webServer>
